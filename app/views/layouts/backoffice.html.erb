<!DOCTYPE html>
<html lang="en">
    <head>
        <title>Backoffice - Virtualstore</title>
        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
        <%= stylesheet_link_tag    'backoffice', media: 'all', 'data-turbolinks-track' => true %>
        <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
        <%= javascript_include_tag 'backoffice', 'data-turbolinks-track' => true %>
        <%= csrf_meta_tags %>
    </head>
    <body>
        <div id="wrapper">
          <%= render partial: "layouts/backoffice_shared/navigation" %>
          <%= yield %>
        </div>
    </body>
</html>
